
/* SVG Morph */
.morph-wrap {
	position: sticky;
	top: 0;
	left: 0;
	bottom: 0;
	width: 100%;
  height: 100vh;
	overflow: hidden;
	display: flex;
	align-items: center;
	justify-content: center;
	pointer-events: none;
  z-index: -1;
}

.morph {
	position: relative;
	height: 42%;
	fill: rgba(#4a90e2, 0.05);
	flex: none;
  top: 15%;
}
